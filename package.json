{
  "name": "angular-demo-app-starter",
  "version": "1.0.0",
  "description": "Angular Demo Application to show some cool stuff :-)",
  "license": "MIT",
  "jest-junit": {
    "suiteName": "angular-demo-app",
    "output": "./tmp/junit.xml"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:all": "ng serve --proxy-config config/proxy.conf.json --configuration hmr & npm run start:server",
    "start:server": "node server",
    "start:client": "ng serve --proxy-config config/proxy.conf.json --configuration hmr",
    "start:dist": "ng build --prod && DIST=true node server",
    "build": "ng build --prod",
    "build:report:es5": "ng build --stats-json && webpack-bundle-analyzer dist/stats-es5.json",
    "build:report:es2015": "ng build --stats-json && webpack-bundle-analyzer dist/stats-es2015.json",
    "jest": "jest --silent",
    "tdd": "jest --bail --expand --verbose",
    "tdd:cover": "jest --coverage --no-cache",
    "test": "ng test",
    "test:cover": "jest --silent --coverage --no-cache && open-cli coverage/index.html",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test:watch": "jest --watch",
    "check": "ncu"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.2.7",
    "@angular/cdk": "8.2.0",
    "@angular/common": "8.2.7",
    "@angular/compiler": "8.2.7",
    "@angular/core": "11.0.5",
    "@angular/forms": "8.2.7",
    "@angular/material": "8.2.0",
    "@angular/platform-browser": "8.2.7",
    "@angular/platform-browser-dynamic": "8.2.7",
    "@angular/router": "8.2.7",
    "core-js": "2.6.5",
    "rxjs": "6.5.3",
    "tslib": "1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.5",
    "@angular/cli": "8.3.5",
    "@angular/compiler-cli": "8.2.7",
    "@angular/language-service": "8.2.7",
    "@angularclass/hmr": "2.1.3",
    "@types/core-js": "2.5.2",
    "@types/jasmine": "3.4.0",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "12.7.5",
    "body-parser": "1.19.0",
    "codelyzer": "5.1.1",
    "compression": "1.7.4",
    "express": "4.17.1",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "jest": "24.9.0",
    "jest-preset-angular": "6.0.2",
    "karma": "4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "karma-mocha-reporter": "2.2.5",
    "material-design-icons": "3.0.1",
    "npm-check-updates": "^3.1.23",
    "open-cli": "5.0.0",
    "protractor": "~5.4.2",
    "ts-node": "~8.4.1",
    "tslint": "~5.20.0",
    "typescript": "3.5.3",
    "webpack-bundle-analyzer": "3.5.0"
  }
}
